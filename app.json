{
  "expo": {
    "name": "sample",
    "slug": "sample",
    "scheme": "sample-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDs6dddB4WI6-2C2XIPIRY1Lqdc64BuwZk"
        }
      },
      "package": "com.nisha0707.sample"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": 
    [
      "expo-router",
      [
      "expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
      }
    ]
    ],
 
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e04ffbdc-a4ad-4b86-a3ec-89edaf9601d8"
      }
    }
  }
}
